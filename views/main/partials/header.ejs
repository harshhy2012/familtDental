<body>

    <!-- Modal -->
    <div class="modal fade" id="appointmentModal" tabindex="-1" role="form" aria-labelledby="appointmentModalTitle"
        aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable modal-lg" role="form">
            <div class="modal-content">
                <div class="modal-header px-3 pt-3">
                    <h5 class="modal-title px-5">Book an Appointment Now!</h5>
                    <button type="button" id="closeMain" class="btn btn-shift" data-bs-dismiss="modal" aria-label="Close"><i
                            class="far fa-times-circle fa-2x btn-shift"></i></button>
                </div>
                <div class="modal-body justify-content-center">
                    <div class="form-section p-5">
                        <form class="form" id="formID">
                            <div class="form-floating">

                                <input type="text" placeholder="Your Name" name="userName" class="form-control"
                                    id="your_name">
                                <label for="floatingInput"><i class="fas fa-user"></i> Name</label>
                                <i class="fas fa-check-circle"></i>
                                <i class="fas fa-exclamation-circle"></i>
                                <small>Error Message</small>
                            </div>
                            <div class="form-floating ">
                                <input type="text" placeholder="0123456789" name="phoneNumber" class="form-control"
                                    id="phone">
                                <label for="phone"><i class="fas fa-phone-alt"></i> Phone
                                    Number</label>
                                <i class="fas fa-check-circle"></i>
                                <i class="fas fa-exclamation-circle"></i>
                                <small>Error Message</small>
                            </div>
                            <div class="form-floating">
                                <input type="text" placeholder="abc@def.com" name="emailID" class="form-control"
                                    id="email">
                                <label for="email"><i class="fas fa-envelope"></i> Email
                                    address</label>
                                <i class="fas fa-check-circle"></i>
                                <i class="fas fa-exclamation-circle"></i>
                                <small>Error Message</small>
                            </div>
                            <div class="form-floating">
                                <textarea rows=30 class="form-control" name="message"  id = "message"
                                    placeholder="Your Message.."></textarea>
                                <label for="message">Your Message</label>
                            </div>
                        </form>
                    </div>
                </div>
                <div class="modal-footer pe-5">
                    <button type="submit" class="btn btn-primary submit-btn  me-4" id="submit-modal" 
                        form="formID">Submit</button>
                </div>
            </div>
        </div>
    </div>

    <!-- Success Message -->
    <div class="modal fade" id="successModal" aria-hidden="true" aria-labelledby="successModalToggle"
        tabindex="-1">
        <div class="modal-dialog modal-dialog-centered modal-xl success-modal">
            <div class="modal-content success-modal">
                <div class="modal-header" id="SuccessModalHeading">
                   <button id = "successButtonClose" class="hide" data-bs-target="#successModal" data-bs-dismiss="modal"></button>
                    <h2 class="modal-title" id="successModalToggle"><i class="far fa-check-circle"></i> Thank you,
                        your
                        response has been recorded, we will contact you shortly!</h2>
                    <button type="button" class="btn" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-footer">
                    
                </div>
            </div>
        </div>
    </div>

    <!-- failure Message -->
    <div class="modal fade" id="failureModal" aria-hidden="true" aria-labelledby="successModalToggle"
        tabindex="-1">
        <div class="modal-dialog modal-dialog-centered modal-lg success-modal">
            <div class="modal-content success-modal">
                <div class="modal-header">
                    <button id = "failureButtonClose" class="hide" data-bs-target="#failureModal" data-bs-dismiss="modal"></button>
                    <h2 class="modal-title" id="successModalToggle"><i class="far fa-times-circle fa-2x"></i>
                        Something
                        Went Wrong! Please try again Later</h2>
                    <button type="button" class="btn" id="modal-close" data-bs-dismiss="modal"
                        aria-label="Close"></button>
                </div>
            </div>
        </div>
    </div>
 
    <button id = "successButton" class="hide btn-hide" data-bs-toggle="modal" data-bs-target="#successModal" data-target="#successModal" data-bs-dismiss="#appointmentModal"></button>
    <button id = "failureButton" class="hide btn-hide" data-bs-toggle="modal" data-bs-target="#failureModal" data-target="#successModal" data-bs-dismiss="#appointmentModal"></button>
    
   

